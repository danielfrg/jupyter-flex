        "@babel/preset-react": "^7.0.0",
        "@danielfrg/illusionist": "^0.2.3",
        "@nteract/mathjax": "^4.0.9",
        "@nteract/outputs": "^3.0.9",
        "@nteract/presentational-components": "^3.3.14",
        "@primer/css": "^14.4.0",
