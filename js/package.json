{
    "name": "@danielfrg/jupyter-flex",
    "version": "0.7.4",
    "description": "Build dashboards using Jupyter Notebooks",
    "author": "Daniel Rodriguez",
    "license": "Apache-2.0",
    "homepage": "https://github.com/danielfrg/jupyter-flex",
    "repository": {
        "type": "git",
        "url": "https://github.com/danielfrg/jupyter-flex/js"
    },
    "bugs": {
        "url": "https://github.com/danielfrg/jupyter-flex/issues"
    },
    "keywords": [
        "jupyter",
        "notebook",
        "dashboards"
    ],
    "publishConfig": {
        "access": "public"
    },
    "source": "src/index.js",
    "module": "lib/index.js",
    "main": "lib/index.js",
    "files": [
        "lib",
        "dist"
    ],
    "scripts": {
        "dev": "npm run clean && webpack --mode=development --watch",
        "build": "npm run clean && webpack --mode=production",
        "build:lib": "babel src/ --out-dir lib",
        "build:all": "npm run build && npm run build:lib",
        "build:watch": "npm run clean && webpack --mode=production --watch",
        "fmt": "prettier --write \"src/**/*.{js,jsx}\"",
        "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
        "clean": "rm -rf .cache/ dist/ lib/",
        "reset": "npm run clean && rm -rf node_modules"
    },
    "peerDependencies": {
        "@danielfrg/illusionist": "^0.3.0",
        "@material-ui/core": "^4.12.3",
        "@material-ui/icons": "^4.11.2",
        "@nteract/mathjax": "^4.0.9",
        "@nteract/outputs": "^3.0.9",
        "@nteract/presentational-components": "^3.3.14",
        "react": "^16.8.0",
        "react-dom": "^16.8.0",
        "react-router-dom": "^5.3.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/eslint-parser": "^7.15.7",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/preset-env": "^7.10.2",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.1.0",
        "css-loader": "^3.5.3",
        "eslint": "^7.2.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.20.0",
        "eslint-plugin-react-hooks": "^4.0.4",
        "file-loader": "^6.0.0",
        "filemanager-webpack-plugin": "^6.1.7",
        "mini-css-extract-plugin": "^0.11.3",
        "null-loader": "^4.0.0",
        "prettier": "^2.0.5",
        "react-test-renderer": "^16.13.1",
        "sass": "^1.26.8",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.1",
        "styled-components": "^5.2.0",
        "url-loader": "^4.1.0",
        "webpack": "^5.0.0",
        "webpack-bundle-analyzer": "^3.9.0",
        "webpack-cli": "^4.8.0",
        "webpack-fix-style-only-entries": "^0.5.1"
    }
}
